{
    "restartable": "rs",
    "exec": "npx kill-port 3010 && npm run devbabel",
    "verbose": true,
    "colours": true,
    "watch": [
        "src/*"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ignore": [
        ".git",
        "tests/**/*",
        "**/__tests__/**",
        "*.test.js"
    ],
    "delay": "2500",
    "ext": "js,json"
}
